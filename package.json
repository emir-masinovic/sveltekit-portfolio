{
	"name": "svelte-portfolio",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"gh-pages": "npm run build && npx gh-pages -d build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.49.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.26.0",
		"gh-pages": "^6.1.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"svelte": "^5.45.4",
		"svelte-check": "^3.0.1",
		"vite": "^7.2.6"
	},
	"type": "module",
	"dependencies": {
		"@babylonjs/core": "^6.31.0",
		"@babylonjs/gui": "^6.31.0",
		"@babylonjs/havok": "^1.3.0",
		"@babylonjs/loaders": "^6.31.0",
		"@babylonjs/materials": "^6.30.0",
		"cannon": "github:schteppe/cannon.js"
	}
}
